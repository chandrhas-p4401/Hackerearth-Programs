#include <stdio.h>
#include<string.h>
int main(){
	char a[200];
	int x=0,y=0,i;
    
	//for(i=0;i<=200;i++)
	//{
	    scanf("%s",&a);
	//}
	for(i=0;a[i]!='\0';i++)
	{
	    if(a[i]=='L')
	    {
	        x-=1;
	        
	    }
	    if(a[i]=='R')
	    {
	        x+=1;
	        
	    }
	    if(a[i]=='U')
	    {
	        
	        y+=1;
	    }
	    if(a[i]=='D')
	    {
	        
	        y-=1;
	    }
	}
	printf("%d %d",x,y);
}
