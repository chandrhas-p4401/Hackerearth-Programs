#include<stdio.h>
#include<string.h>
int main()
{
    
    char arr1[10000];
    char arr2[10000];
    int i,j,c=0,n,a,b,t,k;
    
        scanf("%d",&t);
    while(t)
    {
        c=0;
        a=0;
     scanf("%s",&arr1);
    //gets(arr1);
    //gets(arr2);
    scanf("%s",&arr2);
    for(k=0;arr1[k]!='\0';k++)
    {
        a++;
    }
    b=strlen(arr2);
    for(i=0;i<a;i++)
    {
        for(j=0;j<b;j++)
        {
            if(arr1[i]==arr2[j])
              {
                  c++;
                  arr2[j]=0;
                  break;
              }      
        }
    }
    n=a+b-(c*2);
    printf("%d\n",n);
    t--;
    }
    //elements not make anagram
    
}
